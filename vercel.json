{
    "version": 2,
    "rewrites": [
        {
            "source": "./build/(.*)",
            "destination": "./build"
        }
    ],
    "builds": [
        {
            "src": "./build/index.js",
            "use": "@now/node",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "./build(.*) ",
            "dest": "./build/index.js"
        }
    ]
}